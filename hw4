1- SELECT DISTINCT replacement_cost FROM film;
2- SELECT COUNT(DISTINCT replacement_cost) FROM film;
3- SELECT COUNT(film_name) FROM film WHERE film_name LIKE 'T%' AND rating='G';
4- SELECT COUNT(country_name) FROM COUNTRY  WHERE country_name LIKE '_____'
5- SELECT COUNT(city_name) FROM city WHERE city_name ILIKE '%r';
